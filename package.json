{
  "name": "bakery-swap-pool-v2",
  "version": "1.0.0",
  "license": "MIT",
  "engines": {
    "node": ">=12"
  },
  "files": [
    "artifacts",
    "contracts"
  ],
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-etherscan": "^2.1.4",
    "@nomiclabs/hardhat-solhint": "^2.0.0",
    "@openzeppelin/contracts": "3.4.1",
    "@openzeppelin/test-environment": "^0.1.9",
    "@openzeppelin/test-helpers": "^0.5.12",
    "@typechain/ethers-v5": "^7.0.1",
    "@typechain/hardhat": "^2.2.0",
    "@types/chai": "^4.2.21",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.4.3",
    "bignumber.js": "^9.0.1",
    "bluebird": "^3.7.2",
    "chai": "^4.3.4",
    "ethers": "^5.4.2",
    "hardhat": "^2.5.0",
    "hardhat-abi-exporter": "^2.2.1",
    "hardhat-contract-sizer": "^2.0.3",
    "hardhat-deploy": "^0.8.11",
    "hardhat-deploy-ethers": "^0.3.0-beta.10",
    "hardhat-docgen": "^1.1.1",
    "hardhat-gas-reporter": "^1.0.4",
    "hardhat-watcher": "^2.1.1",
    "mocha": "^9.0.3",
    "prettier": "^2.3.2",
    "prettier-plugin-solidity": "^1.0.0-beta.17",
    "rimraf": "^3.0.2",
    "solhint": "^3.3.6",
    "solidity-coverage": "^0.7.16",
    "ts-generator": "^0.1.1",
    "ts-node": "^10.1.0",
    "typechain": "^5.1.2",
    "typescript": "^4.3.5"
  },
  "scripts": {
    "lint": "yarn prettier --check ./{contracts,scripts,test}  ./*.ts",
    "lint:fix": "yarn prettier --write ./contracts ./*.ts",
    "deploy:contracts": "npx hardhat run scripts/deploy.ts",
    "precompile": "rimraf ./artifacts/ ./cache/",
    "prebuild": "rimraf ./typechain/ && sh ./fix-package.sh",
    "build": "yarn compile",
    "compile": "npx hardhat compile --config hardhat.config.ts  --show-stack-traces",
    "pretest": "yarn compile",
    "test": "npx hardhat test",
    "deploy:bsct": "npx hardhat run --network bsct scripts/deploy.ts",
    "deploy:bsc": "npx hardhat run --network bsc scripts/deploy.ts"
  }
}
